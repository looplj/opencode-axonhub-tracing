{
  "name": "opencode-axonhub-tracing",
  "version": "1.1.0",
  "description": "OpenCode plugin for generic trace header injection (default AxonHub headers)",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build index.ts --outdir dist --target node && bunx tsc -p tsconfig.build.json",
    "typecheck": "bunx tsc --noEmit",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "opencode",
    "plugin",
    "tracing",
    "axonhub",
    "headers",
    "llm"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/looplj/opencode-axonhub-tracing.git"
  },
  "bugs": {
    "url": "https://github.com/looplj/opencode-axonhub-tracing/issues"
  },
  "homepage": "https://github.com/looplj/opencode-axonhub-tracing#readme",
  "peerDependencies": {
    "@opencode-ai/plugin": ">=1.0.0",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@opencode-ai/plugin": "^1.0.150",
    "@types/bun": "^1.3.1",
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3"
  }
}
